provider "aws" {
  region = "us-east-1"
}

resource "aws_db_instance" "mydb" {
  identifier              = "my-mysql-db"
  allocated_storage       = 20
  storage_type            = "gp2"
  engine                  = "mysql"
  engine_version          = "5.7" 
  instance_class          = "db.t3.micro" 
  db_name                 = "mydb"
  username                = "dbadmin"
  password                = "dbadmin123"
  
}
